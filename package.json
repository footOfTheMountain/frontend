{
  "name": "fotm",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "env-cmd -f .env.local next build",
    "local": "env-cmd -f .env.local next dev -p 3000",
    "build:dev": "env-cmd -f .env.dev next build",
    "dev": "env-cmd -f .env.dev next start -p 4000",
    "build:prod": "env-cmd -f .env.prod next build",
    "prod": "env-cmd -f .env.prod next start -p 5000"
  },
  "dependencies": {
    "env-cmd": "^10.1.0",
    "next": "14.2.5",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^8",
    "eslint-config-next": "14.2.5",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
